{
  "name": "fe-code-challenge",
  "version": "1.0.0",
  "description": "Livongo's Front End engineering code challenge.",
  "author": "Livongo",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "scripts": {
    "db": "json-server --watch back-end/db.json --port 9000 --routes back-end/routes.json",
    "start": "concurrently \"npm run db\" \"react-scripts start\""
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "json-server": "^0.16.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "react-scripts": "4.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
